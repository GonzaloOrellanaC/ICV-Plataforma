{
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "Fullstack boilerplate kauel",
    "main": "server/server.js",
    "scripts": {
        "server": "npm run start-dev --prefix server",
        "client": "npm run start --prefix client",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "install:server": "cd server && npm install",
        "install:client": "cd client && npm install",
        "postinstall": "npm run install:server && npm run install:client",
        "prestart": "npm install && npm run build:client",
        "start": "npm run start --prefix server"
    },
    "author": "Javier Romero <javier.romero@kauel.com>",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^6.3.0"
    },
    "dependencies": {
        "express-fileupload": "^1.2.1",
        "node-fetch": "^3.1.0",
        "react-slick": "^0.28.1",
        "slick-carousel": "^1.8.1"
    }
}
